{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "never",
                    "typeLiterals": "ignore"
                },
                "esSpecCompliant": true
            }
        ],
        "semicolon": [
            true,
            "always"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "indent": [
            true,
            "spaces",
            2
        ],
        "ordered-imports": false
    },
    "rulesDirectory": []
}